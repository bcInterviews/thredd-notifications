{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "SmtpConfiguration": {
    "UseSsl": true,
    "Host": "smtpserver.com",
    "Port": 100,
    "WaitBetweenRetriesToThePowerOfInSeconds": 1,
    "RetryCount": 3
  }
}
